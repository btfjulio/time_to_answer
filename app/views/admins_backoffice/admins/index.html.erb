<h1>Administradores</h1>
<div class="table-responsive">
    <table class="table table-striped table-bordered table-hover">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Email</th>
            <th scope="col">Editar/Apagar</th>
        </tr>
    </thead>
    <tbody>
        <% @admins.each do |admin| %>
            <tr>
            <th scope="row">  <%= admin.id %> </th>
            <td><%= admin.email %></td>
            <td style="width: 100px; text-align: center">
                <%= link_to edit_admins_backoffice_admin_path(admin), class:"btn btn-info btn-circle btn-sm"  do %>
                    <i class="fa fa-edit"></i>
                <% end %>
                <%= link_to edit_admins_backoffice_admin_path(admin), class:"btn btn-danger btn-circle btn-sm"  do %>
                    <i class="fa fa-minus"></i>
                <% end %>
            </td>
            </tr>
        <% end %>
    </tbody>
    </table>
</div>